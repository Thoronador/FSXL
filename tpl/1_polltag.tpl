<div id="contentheader">
	<div id="contentlogo">
		<div style="float:right;">{fromdate} - {todate}</div>
		{question}
	</div>
</div>
<div id="contentwindow">
	<table border="0" width="100%" cellpadding="0" cellspacing="2">
		<-- answer -->
		<tr>
			<td class="alt{altnum}"><div style="background-color:#666666; height:5px; width:{width}px;"></div></td>
			<td class="alt{altnum}">{answer}</td>
			<td align="center" class="alt{altnum}">{hits}</td>
			<td align="center" class="alt{altnum}">{percent} %</td>
		</tr>
		<-- /answer -->
	</table>
</div>